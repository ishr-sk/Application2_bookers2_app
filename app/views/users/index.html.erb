<%= render 'users/user_info', user: @user %>

<%= render 'books/book_new', book: @book %>

<div>
  <h1>Users</h1>
  <table>
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
        <th></th>
        <th colspan="3"></th>
      </tr>
    </thead>
    
    <tdoby>
      <% @users.each do |user| %>
        <tr>
          <td>
            <%= attachment_image_tag user, :profile_image, :fill, 50, 50, format: 'jpg', fallback: "no_image.jpg" %>
          </td>
          <td><%= user.name %></td>
          <td><%= link_to "Show" , user_path(user.id) %></td>
        </tr>
      <% end %>
    </tdoby>
    
  </table>
</div>